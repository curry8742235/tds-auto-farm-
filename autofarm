-- // CONFIGURATION
getgenv().AutoStrat = true
getgenv().AutoSkip = true 
getgenv().AutoPickups = true
getgenv().Webhook = "" 

-- // 1. LOAD LIBRARY
local libURL = "https://gist.githubusercontent.com/curry8742235/38ad26d4b4b3373a9b7b21bf59d14015/raw/85d4d1a32dd0f37fb9e2766bcaf97480cdf121c7/library"
local success, TDS = pcall(function() return loadstring(game:HttpGet(libURL))() end)

if not success or not TDS then
    warn("Gist failed, using backup library...")
    TDS = loadstring(game:HttpGet("https://raw.githubusercontent.com/DuxiiT/auto-strat/refs/heads/main/Library.lua"))()
end

if not TDS then 
    game.StarterGui:SetCore("SendNotification", {Title="Error", Text="Library Failed!", Duration=5})
    return 
end

-- // 2. ROBUST MAP ANCHORING (FIXED)
local function GetPositionFromObject(obj)
    if not obj then return nil end
    
    -- If it's a Part, return Position
    if obj:IsA("BasePart") then return obj.Position end
    
    -- If it's a Folder or Model, look inside for "0", "1", or any Part
    if obj:IsA("Folder") or obj:IsA("Model") then
        local child0 = obj:FindFirstChild("0")
        local child1 = obj:FindFirstChild("1")
        
        if child0 and child0:IsA("BasePart") then return child0.Position end
        if child1 and child1:IsA("BasePart") then return child1.Position end
        
        -- Recursive check if children are folders
        if child0 then return GetPositionFromObject(child0) end
        
        -- Fallback: Find ANY part inside
        for _, child in ipairs(obj:GetChildren()) do
            if child:IsA("BasePart") then return child.Position end
        end
    end
    return nil
end

local function GetMapAnchor()
    local map = workspace:FindFirstChild("Map")
    if not map then return Vector3.zero end
    
    -- Priority 1: EnemySpawn
    if map:FindFirstChild("EnemySpawn") then 
        return map.EnemySpawn.Position 
    end
    
    -- Priority 2: Paths (Handles Folders/Models)
    local paths = map:FindFirstChild("Paths")
    if paths then
        -- Check common path names
        local targets = {"0", "1", "Path", "Path1", "Main"}
        for _, name in ipairs(targets) do
            local found = GetPositionFromObject(paths:FindFirstChild(name))
            if found then return found end
        end
        
        -- Fallback: Check any child in Paths
        for _, child in ipairs(paths:GetChildren()) do
            local found = GetPositionFromObject(child)
            if found then return found end
        end
    end
    
    return Vector3.zero
end

-- // 3. APPLY OFFSET
local RecAnchor = Vector3.new(-48.8, 3.8, 14.5) -- Simplicity Spawn
local CurAnchor = GetMapAnchor()
local MapOffset = Vector3.zero

if CurAnchor == Vector3.zero then
    warn("[DeepSeek] ⚠️ Could not find valid Anchor! Units might misplace.")
else
    if (CurAnchor - RecAnchor).Magnitude > 2 then
        MapOffset = CurAnchor - RecAnchor
        print("[DeepSeek] ✅ Adapted to Map! Offset:", MapOffset)
    else
        print("[DeepSeek] ℹ️ No adaptation needed (Same position).")
    end
end

-- Hook Place Function
local oldPlace = TDS.Place
TDS.Place = function(self, name, x, y, z)
    return oldPlace(self, name, x + MapOffset.X, y + MapOffset.Y, z + MapOffset.Z)
end

print("--- STRAT LOADED SUCCESSFULLY ---")

